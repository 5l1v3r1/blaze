PATH_DMATDMATMULT=$( dirname "${BASH_SOURCE[0]}" )

echo " Running dense matrix/dense matrix multiplication tests...";

$PATH_DMATDMATMULT/M2x2aM2x2a; if [ $? != 0 ]; then exit 1; fi
