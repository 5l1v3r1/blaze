$PATH_DMATDMATMULT/MDbM6x6b; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATDMATMULT/MDbM7x5a; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATDMATMULT/MDbM7x5b; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATDMATMULT/MDbM7x9a; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATDMATMULT/MDbM7x9b; if [ $? != 0 ]; then exit 1; fi
