$PATH_DVECDVECADD/VDbV3b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECADD/VDbV4a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECADD/VDbV4b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECADD/VDbV5a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECADD/VDbV5b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECADD/VDbV6a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECADD/VDbV6b; if [ $? != 0 ]; then exit 1; fi
