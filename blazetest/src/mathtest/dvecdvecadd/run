$PATH_DVECDVECADD/V5bV5b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECADD/V5bVDa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECADD/V5bVDb; if [ $? != 0 ]; then exit 1; fi

$PATH_DVECDVECADD/V6aV6a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECADD/V6aV6b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECADD/V6aVDa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECADD/V6aVDb; if [ $? != 0 ]; then exit 1; fi
