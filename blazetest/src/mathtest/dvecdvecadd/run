$PATH_DVECDVECADD/V4bV4a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECADD/V4bV4b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECADD/V4bVDa; if [ $? != 0 ]; then exit 1; fi
