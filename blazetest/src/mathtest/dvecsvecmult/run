PATH_DVECSVECMULT=$( dirname "${BASH_SOURCE[0]}" )

echo " Running dense vector/sparse vector multiplication tests...";

$PATH_DVECSVECMULT/V2aVCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECSVECMULT/V2aVCb; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECSVECMULT/V2bVCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECSVECMULT/V2bVCb; if [ $? != 0 ]; then exit 1; fi

$PATH_DVECSVECMULT/V3aVCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECSVECMULT/V3aVCb; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECSVECMULT/V3bVCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECSVECMULT/V3bVCb; if [ $? != 0 ]; then exit 1; fi

$PATH_DVECSVECMULT/V4aVCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECSVECMULT/V4aVCb; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECSVECMULT/V4bVCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECSVECMULT/V4bVCb; if [ $? != 0 ]; then exit 1; fi

$PATH_DVECSVECMULT/V5aVCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECSVECMULT/V5aVCb; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECSVECMULT/V5bVCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECSVECMULT/V5bVCb; if [ $? != 0 ]; then exit 1; fi

$PATH_DVECSVECMULT/V6aVCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECSVECMULT/V6aVCb; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECSVECMULT/V6bVCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECSVECMULT/V6bVCb; if [ $? != 0 ]; then exit 1; fi

$PATH_DVECSVECMULT/VDaVCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECSVECMULT/VDaVCb; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECSVECMULT/VDbVCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECSVECMULT/VDbVCb; if [ $? != 0 ]; then exit 1; fi

$PATH_DVECSVECMULT/AliasingTest; if [ $? != 0 ]; then exit 1; fi
