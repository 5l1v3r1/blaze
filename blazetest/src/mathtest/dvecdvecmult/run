$PATH_DVECDVECMULT/VDbV2b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECMULT/VDbV3a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECMULT/VDbV3b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECMULT/VDbV4a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECMULT/VDbV4b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECMULT/VDbV5a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECMULT/VDbV5b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECMULT/VDbV6a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECMULT/VDbV6b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECMULT/VDbVDa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECMULT/VDbVDb; if [ $? != 0 ]; then exit 1; fi

$PATH_DVECDVECMULT/AliasingTest; if [ $? != 0 ]; then exit 1; fi
