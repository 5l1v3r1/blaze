PATH_DVECDVECMULT=$( dirname "${BASH_SOURCE[0]}" )

echo " Running dense vector/dense vector multiplication tests...";

$PATH_DVECDVECMULT/V2aV2a; if [ $? != 0 ]; then exit 1; fi
