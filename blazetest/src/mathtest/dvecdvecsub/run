PATH_DVECDVECSUB=$( dirname "${BASH_SOURCE[0]}" )

echo " Running dense vector/dense vector subtraction tests...";

$PATH_DVECDVECSUB/V2aV2a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V2aV2b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V2aVDa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V2aVDb; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V2bV2a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V2bV2b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V2bVDa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V2bVDb; if [ $? != 0 ]; then exit 1; fi

$PATH_DVECDVECSUB/V3aV3a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V3aV3b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V3aVDa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V3aVDb; if [ $? != 0 ]; then exit 1; fi
