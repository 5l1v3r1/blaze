$PATH_DVECDVECSUB/V4aVDa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V4aVDb; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V4bV4a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V4bV4b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V4bVDa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V4bVDb; if [ $? != 0 ]; then exit 1; fi

$PATH_DVECDVECSUB/V5aV5a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V5aV5b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V5aVDa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V5aVDb; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V5bV5a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V5bV5b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V5bVDa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V5bVDb; if [ $? != 0 ]; then exit 1; fi

$PATH_DVECDVECSUB/V6aV6a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V6aV6b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V6aVDa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V6aVDb; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V6bV6a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V6bV6b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V6bVDa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/V6bVDb; if [ $? != 0 ]; then exit 1; fi

$PATH_DVECDVECSUB/VDaV2a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/VDaV2b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/VDaV3a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/VDaV3b; if [ $? != 0 ]; then exit 1; fi
