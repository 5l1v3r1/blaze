$PATH_DVECDVECSUB/VDaVDb; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/VDbV2a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/VDbV2b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/VDbV3a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/VDbV3b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/VDbV4a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/VDbV4b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/VDbV5a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/VDbV5b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/VDbV6a; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/VDbV6b; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/VDbVDa; if [ $? != 0 ]; then exit 1; fi
$PATH_DVECDVECSUB/VDbVDb; if [ $? != 0 ]; then exit 1; fi

$PATH_DVECDVECSUB/AliasingTest; if [ $? != 0 ]; then exit 1; fi
