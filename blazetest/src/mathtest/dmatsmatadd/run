PATH_DMATSMATADD=$( dirname "${BASH_SOURCE[0]}" )

echo " Running dense matrix/sparse matrix addition tests...";

$PATH_DMATSMATADD/M2x2aMCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M2x2aMCb; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M2x2bMCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M2x2bMCb; if [ $? != 0 ]; then exit 1; fi

$PATH_DMATSMATADD/M3x3aMCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M3x3aMCb; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M3x3bMCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M3x3bMCb; if [ $? != 0 ]; then exit 1; fi

$PATH_DMATSMATADD/M4x4aMCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M4x4aMCb; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M4x4bMCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M4x4bMCb; if [ $? != 0 ]; then exit 1; fi

$PATH_DMATSMATADD/M5x5aMCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M5x5aMCb; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M5x5bMCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M5x5bMCb; if [ $? != 0 ]; then exit 1; fi

$PATH_DMATSMATADD/M6x6aMCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M6x6aMCb; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M6x6bMCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M6x6bMCb; if [ $? != 0 ]; then exit 1; fi

$PATH_DMATSMATADD/M7x13aMCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M7x13aMCb; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M7x13bMCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M7x13bMCb; if [ $? != 0 ]; then exit 1; fi

$PATH_DMATSMATADD/M16x8aMCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M16x8aMCb; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M16x8bMCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/M16x8bMCb; if [ $? != 0 ]; then exit 1; fi

$PATH_DMATSMATADD/MDaMCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/MDaMCb; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/MDbMCa; if [ $? != 0 ]; then exit 1; fi
$PATH_DMATSMATADD/MDbMCb; if [ $? != 0 ]; then exit 1; fi

$PATH_DMATSMATADD/AliasingTest; if [ $? != 0 ]; then exit 1; fi
